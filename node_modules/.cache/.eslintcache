Btn.onclick = function(){\n          if((pswrd_1.type == \"password\") && (pswrd_2.type == \"password\")){\n            pswrd_1.type = \"text\";\n            pswrd_2.type = \"text\";\n            this.textContent = \"Hide\";\n            this.classList.add(\"active\");\n          }else{\n            pswrd_1.type = \"password\";\n            pswrd_2.type = \"password\";\n            this.textContent = \"Show\";\n            this.classList.remove(\"active\");\n          }\n        }\n      }else{\n        showBtn.style.display = \"none\";\n      }\n    }\n    return (<>\n    <div class=\"container\">\n         <header>Confirm Password Check in <br/>HTML CSS & JavaScript</header>\n         <form action=\"#\">\n            <div class=\"error-text\"></div>\n            <div class=\"field\">\n               <input onkeyup=\"active()\" id=\"pswrd_1\" type=\"password\" placeholder=\"Enter Password\"/>\n            </div>\n            <div class=\"field\">\n               <input onkeyup=\"active_2()\" id=\"pswrd_2\" disabled type=\"password\" placeholder=\"Confirm Password\"/>\n               <div class=\"show\">\n                  SHOW\n               </div>\n            </div>\n            <button id='buttons' disabled >Check</button>\n         </form>\n      </div>\n    </>)\n}\n\nexport default NewPassword;","/Users/abhijitutkarsh/Documents/site/EcommerceReactFront/src/Components/cart/Cart.js",["111","112","113","114","115","116","117","118","119"],[],"import React, { useEffect, useState } from \"react\";\nimport HeaderHome from \"../Partials/HeaderHome\";\nimport { BrowserRouter as Router, Routes, Route, Link,useNavigate } from \"react-router-dom\";\n\nimport \"./Cart.css\";\nfunction Cart(props) {\n  const [values, setValues] = useState([]);\n  var [addVal, setAddVal] = useState(0);\n  // const [subVal, setsubVal] = useState(0);\n\n  useEffect(() => {\n    console.log(1);\n    if (props.sess.user) {\n      // console.log(props.sess.user)\n      var data = { email: prop